#include "../../../VisOS.Driver/VisOS.TestDevice/test_device.vhl"

string(str_test_name, "Void2ArgsTest");
string(str_ret_val_fail, "Value is not 5");

private void Test(var arg1, var arg2)
{
	var a = arg1 + arg2;
	if(a != 5)
	{
		FailTest(ptr_of(str_ret_val_fail), size_of(str_ret_val_fail));
	}
	return;
}

private void VoidV2ArgsTest()
{
	BeginTest(ptr_of(str_test_name), size_of(str_test_name));
	var ar = 3;
	var br = 2;
	Test(ar,br);
	PassTest();
	return;
}

private void VoidC2ArgsTest()
{
	BeginTest(ptr_of(str_test_name), size_of(str_test_name));
	Test(3,2);
	PassTest();
	return;
}
public void Void2ArgsTest()
{
	VoidC2ArgsTest();
	VoidV2ArgsTest();
	return;
}