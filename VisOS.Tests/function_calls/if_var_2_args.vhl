#include "../../../VisOS.Driver/VisOS.TestDevice/test_device.vhl"

string(str_test_name, "IfVar2ArgsTest");
string(str_ret_val_fail, "Return value is not 5");

private var TestC(var arg1, var arg2)
{
	var a = arg1 + arg2;
	return a;
}

private var TestV(var arg1, var arg2)
{
	return arg1 + arg2;
}

public void IfVar2ArgsTest()
{
	BeginTest(ptr_of(str_test_name), size_of(str_test_name));

	if(TestC(2,3) == 5)
	{
		var a = 3;
		var b = 2;
		if(TestV(a,b) == 5)
		{
			PassTest();
			return;
		}
	}
	FailTest(ptr_of(str_ret_val_fail), size_of(str_ret_val_fail));
	return;
}

IfVar2ArgsTest();