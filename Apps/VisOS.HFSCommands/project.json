{
  "ProjectName": "VisOS.HFSCommands",
  "ProjectVersion": "2021.2.5.1",
  "Dependencies": [],
  "DefaultTarget": "Debug",
  "BuildTargets": {
    "Debug": {
      "TargetName": "Debug",
      "DependsOn": [],
      "Jobs": [
        {
          "JobName": "Restore %NAME%@%VERSION%",
          "BuildJobRunner": "restore",
          "Arguments": {
            "origin": "local"
          }
        },
        {
          "JobName": "Merged Debug Build",
          "BuildJobRunner": "merged",
          "Arguments": {
            "merge:include": "%VISDIR%common/jobs/debug_build.json",
            "build:input": "App.vhl",
            "assembler:offset.global": "0"
          }
        }
      ]
    },
    "Publish": {
      "TargetName": "Publish",
      "DependsOn": [],
      "Jobs": [
        {
          "JobName": "Publish Project",
          "BuildJobRunner": "merged",
          "Arguments": {
            "merge:include": "%VISDIR%common/jobs/newVersion.json"
          }
        }
      ]
    },
    "Release": {
      "TargetName": "Release",
      "DependsOn": [],
      "Jobs": [
        {
          "JobName": "Restore %NAME%@%VERSION%",
          "BuildJobRunner": "restore",
          "Arguments": {
            "origin": "local"
          }
        },
        {
          "JobName": "Merged Release Build",
          "BuildJobRunner": "merged",
          "Arguments": {
            "merge:include": "%VISDIR%common/jobs/release_build.json",
            "build:input": "App.vhl",
            "assembler:offset.global": "4096"
          }
        }
      ]
    }
  }
}