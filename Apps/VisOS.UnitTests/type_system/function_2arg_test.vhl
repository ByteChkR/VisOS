#include "../VisOS.Driver/test_device.vhl"

private static string str_var_test_name= "CTYPE_2VAR_ARGS_Test";
private static string str_const_test_name= "CTYPE_2CONST_ARGS_Test";
private static string str_generic_error= "Generic Error";

class TestClass
{
	public var a;
	public var b;
}

TestClass const_ar;
TestClass const_ab;

private void TestXY(TestClass arg1, TestClass arg2)
{
	if(arg1.b == arg2.b)
	{
		PassTest();
		return;
		
	}
	FailTest(ptr_of(str_generic_error), size_of(str_generic_error));
	return;
}

private void CustomTypeV2ArgsTest()
{
	TestClass ar;
	ar.b = 5;
	TestClass ab;
	ab.b = 5;
	

	BeginTest(ptr_of(str_var_test_name), size_of(str_var_test_name));
	TestXY(ar, ab);
	return;
}

private void CustomTypeC2ArgsTest()
{
	const_ar.b = 5;
	const_ab.b = 5;
	BeginTest(ptr_of(str_const_test_name), size_of(str_const_test_name));
	TestXY(const_ar, const_ab);
	return;
}

public void CustomType2ArgsTest()
{
	CustomTypeC2ArgsTest();
	CustomTypeV2ArgsTest();
	return;
}
CustomType2ArgsTest();