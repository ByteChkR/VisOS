#include "../VisOS.Driver/test_device.vhl"

private static string test_single_field_type= "SINGLE_FIELD_TYPE_TEST";


private static string test_generic_fail= "Type System Error.";


class TestClass
{
	public uint field;
}

public void TestSingleField()
{
	BeginTest(ptr_of(test_single_field_type), size_of(test_single_field_type));
	TestClass tc;
	tc.field = 4;
	if(tc.field == 4)
	{
		PassTest();
		return;
	}
	FailTest(ptr_of(test_generic_fail), size_of(test_generic_fail));
	return;
}
