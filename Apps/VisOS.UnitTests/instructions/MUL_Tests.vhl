}

public static void Run_MUL_Tests()
{
	MUL_VV_Simple_Test();
	MUL_VC_Simple_Test();
	MUL_CV_Simple_Test();
	MUL_CC_Simple_Test();
	return;
}

Run_MUL_Tests();