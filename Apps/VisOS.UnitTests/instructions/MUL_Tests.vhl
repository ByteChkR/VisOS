private static void MUL_CV_Simple_Test()
{
	Tests.Begin(ptr_of(test_mul_cv), size_of(test_mul_cv));
	uint a = 2;
	uint c = 2 * a;
	if(c == 4)
	{
		Tests.Pass();
		return;
	}
	Tests.Fail(ptr_of(test_generic_math_fail), size_of(test_generic_math_fail));
