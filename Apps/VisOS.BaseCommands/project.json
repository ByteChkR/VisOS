{
  "DefaultTarget": "Debug",
  "BuildTargets": {
    "Debug": {
      "TargetName": "Debug",
      "DependsOn": [],
      "Jobs": [
        {
          "JobName": "Create New Version",
          "BuildJobRunner": "combined",
          "Arguments": {
            "newVersion": "%VISDIR%common/jobs/newVersion.json"
          }
        },
        {
          "JobName": "Build %NAME%@%VERSION%",
          "BuildJobRunner": "build",
          "Arguments": {
            "build:input": "%PROJDIR%App.vhl",
            "build:steps": "HL-expr bin",
            "build:clean": "false",
            "assembler:offset.global": "8192",
            "linker:export": "true"
          }
        }
      ]
    }
  },
  "ProjectName": "VisOS.BaseCommands",
  "ProjectVersion": "0.1.0.52",
  "Dependencies": []
}