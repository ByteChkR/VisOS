string(cmd_clear, "clear");

private var IsClear(var ptrIn)
{
	return IsCommand(ptr_of(cmd_clear), size_of(cmd_clear), ptrIn);
}

private void ClearCommand(var argStart, var argLength)
{
	ClearConsole();
	return;
}

public void RegisterClear()
{
	InsertCommand(ptr_of(ClearCommand), ptr_of(IsClear));
	return;
}