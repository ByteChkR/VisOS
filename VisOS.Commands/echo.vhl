#include "../VisOS.Core/command_list.vhl"
#include "../VisOS.Core/const_data.vhl"
#include "VisOS.Tests/tests.vhl"
#include "../VisOS.Driver/VisOS.Terminal/out.vhl"

string(cmd_echo, "echo");

private var IsEcho(var ptrIn)
{
	return IsCommand(ptr_of(cmd_echo), size_of(cmd_echo), ptrIn);
}

private void EchoCommand(var argStart, var argLength)
{
	WriteLine(argStart, argLength);
}

public void RegisterEcho()
{
	InsertCommand(ptr_of(EchoCommand), ptr_of(IsEcho));
	return;
}