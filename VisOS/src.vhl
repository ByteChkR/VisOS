#include "VisOS.Core/startup.vhl"
#include "VisOS.Core/command_loop.vhl"
#include "VisOS.Core/shutdown.vhl"
#include "VisOS.Commands/help.vhl"
#include "VisOS.Commands/test.vhl"
#include "VisOS.Commands/count.vhl"
#include "VisOS.Commands/var_safe.vhl"
#include "VisOS.Commands/echo.vhl"
#include "VisOS.Commands/clear.vhl"
#include "VisOS.Commands/fsize.vhl"
#include "VisOS.Commands/fprint.vhl"
#include "VisOS.Commands/finfo.vhl"


private void RegisterCommands()
{
	
	RegisterHelp();

	RegisterTest();
	
	RegisterCount();

	RegisterVarSafe();

	RegisterEcho();

	RegisterClear();

	RegisterFSize();

	RegisterFPrint();
	
	RegisterFInfo();
	
	return;
}

public void RunOS()
{
	Startup();

	RegisterCommands();

	CommandLoop();
	Shutdown();
	return;
}